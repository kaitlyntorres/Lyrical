<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lyrical">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.lyrical.loginActivity"></activity>
        <activity android:name="com.example.lyrical.reset"></activity>
        <activity android:name="com.example.lyrical.registerActivity"></activity>
        <activity android:name="com.example.lyrical.guestActivity"></activity>
        <activity android:name="com.example.lyrical.displaySong"></activity>
        <activity android:name="com.example.lyrical.spotifySong"></activity>
        <activity android:name="com.example.lyrical.ApiActivity"></activity>
        <activity android:name="com.example.lyrical.f"></activity>
        <activity android:name="com.example.lyrical.h"></activity>






    </application>

</manifest>